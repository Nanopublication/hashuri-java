@prefix pav: <http://swan.mindinformatics.org/ontologies/1.2/pav/> .
@prefix swan: <http://purl.org/swan/1.2/swan-commons/> .
@prefix np: <http://www.nanopub.org/nschema#> .

this: {
	this: a np:Nanopublication ;
		np:hasAssertion sub:Ass ;
		np:hasProvenance sub:Prov ;
		np:hasPublicationInfo sub:Info .
}

sub:Ass {
	<http://example.com/mosquito> <http://example.com/transmits> <http://example.com/malaria> .
}

sub:Prov {
	this: swan:citesAsSupportiveEvidence <http://iospress.metapress.com/content/ftkh21q50t521wm2/> .
}

sub:Info {
	this: pav:authoredBy <http://www.tkuhn.ch/me> ;
		dc:created "2013-02-25T14:33:22+01:00"^^xsd:dateTime .
}
